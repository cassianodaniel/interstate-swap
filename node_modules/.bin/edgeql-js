#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules/edgedb/dist/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules/edgedb/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules/edgedb/dist/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules/edgedb/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/edgedb@1.3.4/node_modules:/Users/danielchavesdev/dev/apmhelp/system-apmhelp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../edgedb/dist/cli.js" "$@"
else
  exec node  "$basedir/../edgedb/dist/cli.js" "$@"
fi
